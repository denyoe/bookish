{
	"name": "literate-quiz",
	"version": "1.0.0",
	"description": "Bookish Quiz API",
	"author": "Marcus Ekon <marcus.ekon@gmail.com>",
	"private": true,
	"scripts": {
		"test": "jest --runInBand --forceExit",
		"watch": "jest --watchAll",
		"dev": "ts-node ./src/server.ts",
		"dev:live": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/server.ts",
		"start": "nodemon ./lib/server.js",
		"build": "tsc -p .",
		"build:live": "tsc --watch",
		"prod": "yarn run build && yarn run start",
		"migrate:start": "sequelize db:migrate && yarn run nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/server.ts"
	},
	"devDependencies": {
		"@types/cors": "^2.8.4",
		"@types/express": "^4.16.0",
		"@types/jest": "^23.3.10",
		"@types/knex": "^0.15.1",
		"@types/morgan": "^1.7.35",
		"@types/node": "^10.12.15",
		"@types/sequelize": "^4.27.38",
		"jest": "^23.6.0",
		"nodemon": "^1.18.9",
		"supertest": "^3.3.0",
		"ts-jest": "^23.10.5",
		"ts-node": "^7.0.1",
		"typescript": "^3.2.2"
	},
	"dependencies": {
		"apollo-server": "^2.4.8",
		"body-parser": "^1.18.3",
		"cors": "^2.8.5",
		"dotenv": "^6.2.0",
		"express": "^4.16.4",
		"graphql": "^14.1.1",
		"knex": "^0.16.3",
		"morgan": "^1.9.1",
		"mysql": "^2.16.0",
		"mysql2": "^1.6.5",
		"sequelize": "^4.42.1",
		"sequelize-cli": "^5.4.0",
		"sqlite3": "^4.0.6"
	}
}
