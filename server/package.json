{
	"name": "literate-quiz",
	"version": "1.0.0",
	"description": "Bookish Quiz API",
	"author": "Marcus Ekon <marcus.ekon@gmail.com>",
	"private": true,
	"scripts": {
		"test": "jest --runInBand --forceExit",
		"watch": "jest --watchAll",
		"dev": "ts-node ./src/server.ts",
		"dev:live": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/server.ts",
		"start": "node ./dist/src/server.js",
		"build": "./node_modules/.bin/tsc -p .",
		"build:live": "tsc --watch",
		"seed": "node ./seeder/run.js",
		"prod": "yarn build && yarn run start",
		"migrate:start": "sequelize db:migrate && yarn run nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/server.ts"
	},
	"devDependencies": {
		"@types/cors": "^2.8.4",
		"@types/express": "^4.16.0",
		"@types/jest": "^23.3.10",
		"@types/mongoose": "^5.3.23",
		"@types/morgan": "^1.7.35",
		"@types/node": "^10.12.15",
		"jest": "^23.6.0",
		"nodemon": "^1.18.9",
		"supertest": "^3.3.0",
		"ts-jest": "^23.10.5",
		"ts-node": "^7.0.1",
		"typescript": "^3.2.2"
	},
	"dependencies": {
		"JSONStream": "^1.3.5",
		"apollo-server": "^2.4.8",
		"body-parser": "^1.18.3",
		"cors": "^2.8.5",
		"dotenv": "^6.2.0",
		"event-stream": "^4.0.1",
		"express": "^4.16.4",
		"graphql": "^14.1.1",
		"mongoose": "^5.4.19",
		"morgan": "^1.9.1"
	}
}
